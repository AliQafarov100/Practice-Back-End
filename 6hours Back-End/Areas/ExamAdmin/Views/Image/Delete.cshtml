@model Image
@{
    ViewData["Title"] = "Delete";
}

<h1>Delete</h1>
<div>
    <div>
        <p>Id</p>
        <h4>@Model.Id</h4>
    </div>
    <div>
        <p>Photo</p>
        <h4>
            <img src="/assets/Image/Cards/@Model.Photo" style="width: 50%; height: 300px;"/>
        </h4>
    </div>
</div>

<form method="post">
    <div class="mt-3">
        <button type="submit" class="btn btn-danger">Delete Image</button>
    </div>
</form>